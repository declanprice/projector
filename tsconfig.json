{
  "include": ["src"],
  "compilerOptions": {
    "target": "es2020",
    "module": "es2020",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "dist",
  },
  "exclude": [
    "node_modules",
    "cdk.out",
    "app",
    "dist"
  ]
}
